{
  "name": "adonis-fcm",
  "version": "1.0.1-rc1",
  "description": "Firebase Cloud Messaging for AdonisJS",
  "main": "./build/providers/FCMProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/templates",
    "build/src"
  ],
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-fcm.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-fcm",
    "adonis-firebase",
    "adonis-cloud-messaging",
    "adonis-push-notification"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-fcm/issues"
  },
  "dependencies": {
    "node-gcm": "^1.0.3"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc-2",
    "@adonisjs/mrm-preset": "^2.4.0",
    "@types/node": "^14.14.6",
    "@types/node-gcm": "^1.0.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.12.1",
    "eslint-plugin-adonis": "^1.0.15",
    "japa": "^3.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "directories": {
    "test": "test"
  },
  "adonisjs": {
    "templates": {
      "config": [
        "fcm.txt"
      ]
    },
    "env": {
      "FCM_API_KEY": ""
    },
    "types": "adonis-fcm",
    "providers": [
      "adonis-fcm"
    ]
  }
}
